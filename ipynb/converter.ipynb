    "def to_hour(seconds):\n",
    "    seconds = seconds % (24 * 3600) #86400\n",
    "    hour = seconds // 3600\n",
    "    seconds %= 3600\n",
    "    minutes = seconds // 60\n",
    "    seconds %= 60\n",
    "    return \"%d:%02d:%02d\" % (hour, minutes, seconds)\n",
    "\n",
    "\n",
    "print(1000/69)\n",
    "print(1000//69)\n",
    "print(1000%69)\n",
    "\n",
    "print(to_hour(1000))"
   ]
  }
 ],
